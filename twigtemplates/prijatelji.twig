{% extends "base.twig" %}

{% block content %}

    <div class="prijatelji">

        <div class="titles" style="margin-bottom: 3%;">Prijatelji SUZI</div>
        <div class="prijatelji-boks">
            {% set Prijatelji = fetch("Prijatelji") %}
            {% for item in Prijatelji %}
            <div class="prijatelji-boks--1" style="display: block;">
                <div class="prijatelji-boks--1--logo">
                    <img src="img/{{item.Logo}}">
                </div>
                <div class="prijatelji-boks--1--adresa">{{item.Adresa}} </div>
                <div class="prijatelji-boks--1--link"><a href="{{item.Link}} ">{{item.Link}} </a></div>
            </div>
            {% endfor %}

        </div>

    </div>

{% endblock %}